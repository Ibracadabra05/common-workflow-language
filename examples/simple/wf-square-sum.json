    },
    {
      "id": "#square",
      "impl": "mul.json",
      "inputs": [
        {
          "id": "#square/a",
          "links": {"source": "#sum/sum"}
        },
        {
          "id": "#square/b",
          "links": {"source": "#sum/sum"}
        }
      ],
      "outputs": {"id": "#square/c"}
    }
  ]
}
